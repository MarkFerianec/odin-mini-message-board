<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Home</title>
  </head>
  <body>
    <h1>Home</h1>
    <%- include('navbar', {links: links}) %>
    <ul>
      <% messages.forEach((message) => { %>
      <li>
        User: <%= message.user %> Message: <%= message.text %> Date added: <%=
        message.added %>
      </li>
      <% }) %>
    </ul>
  </body>
</html>
