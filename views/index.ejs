<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Home</title>
  </head>
  <body>
    <h1>Home</h1>
    <%- include('navbar', {links: links}) %>
    <ul>
      <% messages.forEach((message) => { %>
      <li>
        <%= message.username %>: <%= message.message %> <%= message.added %>
        <a href="<%= ((messages.indexOf(message)) + 1) %>">Open</a>
      </li>
      <% }) %>
    </ul>
  </body>
</html>
